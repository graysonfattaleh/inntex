@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">You do not have access to this resource. If you feel you should have access to this resource, please contact a head researcher who has Super Admin privileges.</p>
</header>
